{
	"build": {
		"env": {
			"HUGO_VERSION": "0.105.0"
		}
	},
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=31536000; includeSubDomains; preload"
				},
				{
					"key": "X-Frame-Options",
					"value": "SAMEORIGIN"
				},
				{
					"key": "Referrer-Policy",
					"value": "strict-origin"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Powered-By",
					"value": "Hugo"
				}
			]
		}
	]
}
